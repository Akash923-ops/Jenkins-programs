pipeline{
    agent any
    stages{
        stage('checkout'){
            steps{
 checkout scmGit(branches: [[name: '*/main']], 
 extensions: [], userRemoteConfigs: [[credentialsId: 'git_up',
  url: 'https://github.com/Akash923-ops/java_app.git']])
               
               sh '''
               cd ./calculator_app
               '''
            }
            }
        
         stage('test'){
            steps{
                sh 'ls -rt'
            }
         }
    }
}