pipeline{
    any agent
    stages{
        stage('code checkout'){
            steps{
                  git branch: 'main', credentialsId: 'git-id', url: 'https://github.com/Akash923-ops/my-devops-2024.git'
            }
        }
          stage('check file'){
            steps{
                sh 'ls -lrt'
            }
          }
    }
}